function GeInterface()

global LAIdx;
LAIdx = 1;
GeFig = figure;

set(GeFig,...
     'MenuBar','none',...
     'Name','GoogleEarth',...
     'NumberTitle','off',...
     'Position', [0 50 810 710]);

uicontrol(...
    'Parent',GeFig,...            
    'BackgroundColor',[0.2 0.2 0.2],...
    'Callback',@NextCamCallback,...
    'CData',imread('NextCam.png'),...
    'Position',[407.5 655 50 50],...
    'String',blanks(0),...
    'TooltipString','Get the view from the next camera',...
    'Style','pushbutton');

uicontrol(...
    'Parent',GeFig,...            
    'BackgroundColor',[0.2 0.2 0.2],...
    'Callback',@PrevCamCallback,...
    'CData',imread('PrevCam.png'),...
    'Position',[352.5 655 50 50],...
    'String',blanks(0),...
    'TooltipString','Get the view from the previous camera',...
    'Style','pushbutton');

end


function PrevCamCallback(~,~)
    global LAIdx
    LAIdx = LAIdx -1;

end

function NextCamCallback(~,~)
    global LAIdx
    LAIdx = LAIdx +1;

end