

tmp = Kuka;
dloc = EventDispatcher('127.0.0.1',9527);
initloc = [-122.4,37.8];

figure;
hold on;

for i = 1:100
    tmploc = tmp.location;
    plot(tmploc(1),tmploc(2),'*');    
    
    dloc.dispatch(sprintf('1,123456,%f7,%f7,%f7',10*rand,initloc(1),initloc(2)));
