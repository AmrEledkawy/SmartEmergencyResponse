classdef ItemFactory
    %ITEMFACTORY 
    
    properties (Constant)
        itemTable = cell2struct(...
            {'Defibrillator',    'b',   1,        'Quadcopter',    1,        5;...
             'Thermal Camera',   'y',   1,        'Quadcopter',    1,        4;...
             'Mobile Camera',    'w',   1,        'Quadcopter',    1,        3;...
             'Stabilizer',       'y',   1,        'Quadcopter',    1,        5;...
             'Sterile Band',     'r',   1,        'Quadcopter',    1,        2;...
             'Thrombolytics',    'k',   1,        'Quadcopter',    1,        2;...
             'Thermal Blanket',  'c',   1,        'Quadcopter',    1,        2;...
             'Carbamazepine',    'w',   1,        'Quadcopter',    1,        2;...
             'Oral Airway',      'b',   1,        'Quadcopter',    1,        2;...
             'Endotracheal Tube','y',   1,        'Quadcopter',    1,        2;...
             'Mask',             'w',   1,        'Quadcopter',    1,        3;...
             'Personal Drone',   'm',   1,        'Quadcopter',    1,        3;...
             'Sensory Animal',   'm',   1,        'Car',           1,        3;...
             'Robotic Arm',      'm',   1,        'Car',           1,        3;...
             'Robot ATLAS',      'm',   1,        'Car',           1,        3;...
             'Valve',            'm',   1,        'Car',           1,        3},...
                {'name',      'color' 'priority' 'carrier'      'volume'  'weight'},...
                                2);
    end
    
    methods (Static)        
        function retval = makeItem(name)
            t = ItemFactory.itemTable;   %g835097
            if strcmp(name,'empty')
                retval = t([]);
            else
                retval = t(strcmp({t.name},name));
            end                
        end
        
        function retval = getCatalog
            t = ItemFactory.itemTable;   %g835097
            retval = {t.name};
        end            
    end
    
end
