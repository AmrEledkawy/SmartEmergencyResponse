classdef ItemFactory
    %ITEMFACTORY 
    
    properties (Constant)
        itemTable = cell2struct(...
            {'Defibrillator',    'b',   1,        'Car',    1,          5,     true,        false;...
             'Thermal Camera',   'y',   1,        'Car',    1,          4,     false,       true;...
             'Mobile Camera',    'w',   1,        'Car',    1,          3,     false,       true;...
             'Stabilizer',       'y',   1,        'Car',    1,          5,     false,       true;...
             'Sterile Band',     'r',   1,        'Car',    1,          2,     false,       true;...
             'Thrombolytics',    'k',   1,        'Car',    1,          2,     false,       true;...
             'Thermal Blanket',  'c',   1,        'Car',    1,          2,     false,       true;...
             'Carbamazepine',    'w',   1,        'Car',    1,          2,     false,       true;...
             'Oral Airway',      'b',   1,        'Car',    1,          2,     false,       true;...
             'Endotracheal Tube','y',   1,        'Car',    1,          2,     false,       true;...
             'Mask',             'w',   1,        'Car',    1,          3,     false,       true;...
             'Personal Drone',   'm',   1,        'Car',    1,          3,     false,       true;...
             'Sensory Animal',   'm',   1,        'Car',    1,          3,     false,       true;...
             'Robotic Arm',      'm',   1,        'Car',    1,          3,     false,       true;...
             'Robot ATLAS',      'm',   1,        'Car',    1,          3,     false,       true;...
             'Valve',            'm',   1,        'Car',    1,          3,     false,       true},...
                {'name',      'color' 'priority' 'carrier' 'volume' 'weight' 'isPickup' 'isDelivery'},...
                                2);
    end
    
    methods (Static)        
        function retval = makeItem(name)
            t = ItemFactory.itemTable;   %g835097
            retval = t(strcmp({t.name},name));
        end
        
        function retval = getCatalog
            t = ItemFactory.itemTable;   %g835097
            retval = {t.name};
        end            
    end
    
end
