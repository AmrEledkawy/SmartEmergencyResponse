function [latA, lonA, altA, modeA, headA, tiltA, rangeA] = stringifyLLAMHTR(lat, lon, alt, mode, head, tilt, range)

latA = ['[' num2str(lat(1), '%0.9e')];
for i=2:size(lat) latA = [latA ',' num2str(lat(i), '%0.9e')]; end
latA = [latA ']'];

lonA = ['[' num2str(lon(1), '%0.9e')];
for i=2:size(lon) lonA = [lonA ',' num2str(lon(i), '%0.9e')]; end
lonA = [lonA ']'];

altA = ['[' num2str(alt(1), '%0.9e')];
for i=2:size(alt) altA = [altA ',' num2str(alt(i), '%0.9e')]; end
altA = [altA ']'];

modeA = ['[' num2str(mode(1), '%0.9e')];
for i=2:size(mode) modeA = [modeA ',' num2str(mode(i), '%0.9e')]; end
modeA = [modeA ']'];

headA = ['[' num2str(head(1), '%0.9e')];
for i=2:size(head) headA = [headA ',' num2str(head(i), '%0.9e')]; end
headA = [headA ']'];

tiltA = ['[' num2str(tilt(1), '%0.9e')];
for i=2:size(tilt) tiltA = [tiltA ',' num2str(tilt(i), '%0.9e')]; end
tiltA = [tiltA ']'];

rangeA = ['[' num2str(range(1), '%0.9e')];
for i=2:size(range) rangeA = [rangeA ',' num2str(range(i), '%0.9e')]; end
rangeA = [rangeA ']'];

end