function GeInterface(obj)

GeFig = figure;
obj.LAIdx = 1;

set(GeFig,...
     'MenuBar','none',...
     'Name','GoogleEarth',...
     'NumberTitle','off',...
     'Position', [0 50 810 710]);

uicontrol(...
    'Parent',GeFig,...            
    'BackgroundColor',[0.2 0.2 0.2],...
    'Callback',{@incrLAIdx,obj},...
    'CData',imread('NextCam.png'),...
    'Position',[407.5 655 50 50],...
    'String',blanks(0),...
    'TooltipString','Get the view from the next camera',...
    'Style','pushbutton');

uicontrol(...
    'Parent',GeFig,...            
    'BackgroundColor',[0.2 0.2 0.2],...
    'Callback',{@decrLAIdx,obj},...
    'CData',imread('PrevCam.png'),...
    'Position',[352.5 655 50 50],...
    'String',blanks(0),...
    'TooltipString','Get the view from the previous camera',...
    'Style','pushbutton');

uicontrol(...
    'Parent',GeFig,...            
    'BackgroundColor',[0.2 0.2 0.2],...
    'Callback',{@CameraView,obj},...
    'CData',imread('Camera.png'),...
    'Position',[5 655 50 50],...
    'String',blanks(0),...
    'TooltipString','Switch to Camera View',...
    'Style','pushbutton');

uicontrol(...
    'Parent',GeFig,...            
    'BackgroundColor',[0.2 0.2 0.2],...
    'Callback',{@SpanView,obj},...
    'CData',imread('Hand.png'),...
    'Position',[63 655 50 50],...
    'String',blanks(0),...
    'TooltipString','Switch to Camera View',...
    'Style','pushbutton');

end

function decrLAIdx(~,~,obj)
    if (obj.LAIdx == 1)
        obj.LAIdx = obj.Views; 
    else
        obj.LAIdx = obj.LAIdx -1;
    end
    obj.updateGe();
end

function CameraView(~,~,obj)
     obj.Cr = ~obj.Cr;
     obj.LAIdx = 1;
end

function incrLAIdx(~,~,obj)
    if (obj.LAIdx == obj.Views)
        obj.LAIdx = 1; 
    else
        obj.LAIdx = obj.LAIdx +1;        
    end
    obj.updateGe();
end

function SpanView(src,~,obj)
        switch get(src,'Value')
            case get(src,'Max')                
                stopTimer(obj);
            case get(src,'Min')
                startTimer(obj);
        end

end
